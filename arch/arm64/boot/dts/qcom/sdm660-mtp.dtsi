	mw,gpio-key{
		compatible = "mw,gpio-key";
		mw,irq-pin = <&tlmm 47 0x0>;
		mw,led-pin = <&tlmm 48 0x0>;
		mw,led-pin-volume = <&tlmm 68 0x0>;
		mw,led-pin-mute-white = <&tlmm 69 0x0>;
		mw,led-pin-mute-red = <&tlmm 21 0x0>;
	};
};

&mem_client_3_size {
